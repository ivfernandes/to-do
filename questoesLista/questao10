name: Test Workflow

on: [push]
 branches: 
      - init-front-end

jobs:
  test:
    
    strategy:
      matrix:
        os: [ubuntu-18.04, ubuntu-20.04, ubuntu-22.04]
      runs-on: ${{ matrix.os }}
    steps:
      - name: Checkout code
        uses: actions/checkout@v2

      - name: Install dependencies
        run: echo "npm install"

      - name: Run tests
        run: echo "npm test"